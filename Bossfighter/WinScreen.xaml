<Window x:Class="Bossfighter.WinScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bossfighter"
        mc:Ignorable="d"
        Title="WinScreen" Height="550" Width="800" WindowStartupLocation="CenterScreen">
    <StackPanel Background="#292928">
        <Image Source="/Images/victory.png" Height="434"/>
        <Button Content="Close" Width="110" Height="35" FontSize="25" Background="#bfa3e7" Foreground="#f2f3f2" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Click="Button_Click" ClickMode="Release"/>
    </StackPanel>
</Window>
